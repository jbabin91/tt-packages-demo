{
  "name": "@jbabin91/mui-theme",
  "version": "1.4.3",
  "description": "A demo package for a mui theme",
  "keywords": [
    "demo",
    "mui",
    "theme",
    "typescript"
  ],
  "homepage": "https://github.com/jbabin91/tt-packages-demo/tree/main/packages/theme",
  "bugs": {
    "url": "https://github.com/jbabin91/tt-packages-demo/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbabin91/tt-packages-demo.git"
  },
  "license": "MIT",
  "author": "Jace Babin",
  "type": "module",
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./src/index.ts"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
    "clean": "rm -rf .turbo node_modules dist",
    "dev": "vitest",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/lab": "6.0.0-dev.240424162023-9968b4889d",
    "@mui/material": "^6.5.0",
    "@mui/x-charts": "^7.29.1",
    "@mui/x-data-grid": "^7.29.12",
    "@mui/x-date-pickers": "^7.29.4",
    "@mui/x-tree-view": "^7.29.10",
    "dayjs": "^1.11.18",
    "notistack": "^3.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7"
  }
}
